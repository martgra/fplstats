{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get", "post"],
      "route": "read/gw/{gameweek}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "inputDocument",
      "databaseName": "fplstats",
      "collectionName": "elements",
      "connectionStringSetting": "DB_CONNECTION",
      "sqlQuery": "SELECT * from c where c.gameweek = StringToNumber({gameweek})"
    }
  ]
}
